{
  "name": "ts2gd",
  "version": "0.0.36",
  "description": "TypeScript to GDScript transpiler.",
  "main": "js/main.js",
  "scripts": {
    "publish-local": "npm run tsc && npm link",
    "auto-publish": "npm run tsc && git add . && git commit -m 'include generated JS' && npm version patch && npm publish",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn main.ts",
    "test": "ts-node-dev --clear tests/test.ts",
    "build-ci": "tsc",
    "test-ci": "node js/tests/test.js",
    "project-test": "ts-node-dev --respawn --clear tests/project_tests.ts",
    "lint-warn": "eslint .",
    "lint-validate": "eslint . --max-warnings=0",
    "lint-fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "generate-defs": "ts-node main.ts --buildLibraries --buildOnly mockProject/ts2gd.json"
  },
  "prepublish": "tsc",
  "bin": {
    "ts2gd": "bin/index.js"
  },
  "files": [
    "_godot_defs",
    "bin",
    "js"
  ],
  "author": "johnfn",
  "license": "MIT",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "chalk": "^5.5.0",
    "chokidar": "^4.0.3",
    "diff": "^8.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "pkginfo": "^0.4.1",
    "tsutils": "^3.21.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/pkginfo": "^0.4.3",
    "@types/xml2js": "^0.4.14",
    "anymatch": "^3.1.3",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
